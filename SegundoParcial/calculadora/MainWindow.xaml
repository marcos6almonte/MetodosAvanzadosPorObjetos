        <Button x:Name="Division" Content="Division" HorizontalAlignment="Left" Margin="254,167,0,0" VerticalAlignment="Top" Width="75" Click="Division_Click"/>
        <Button x:Name="ANS" Content="ANS" HorizontalAlignment="Left" Margin="56,230,0,0" VerticalAlignment="Top" Width="75" Click="ANS_Click"/>
        <Button x:Name="Multiplicacion" Content="Multiplicacion" HorizontalAlignment="Left" Margin="349,167,0,0" VerticalAlignment="Top" Width="105" RenderTransformOrigin="0.503,0.103" Click="Multiplicacion_Click"/>
        <Button x:Name="Resta" Content="Resta" HorizontalAlignment="Left" Margin="149,167,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.644,0.428" Click="Resta_Click"/>

    </Grid>
</Window>
