        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_180418_actividadMVC"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="Primero" HorizontalAlignment="Left" Height="23" Margin="44,86,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
